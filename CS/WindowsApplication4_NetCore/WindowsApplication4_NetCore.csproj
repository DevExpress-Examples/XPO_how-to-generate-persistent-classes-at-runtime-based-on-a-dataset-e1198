<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\WindowsApplication4\Form1.cs" Link="Form1.cs" />
    <Compile Include="..\WindowsApplication4\Form1.Designer.cs" Link="Form1.Designer.cs" />
    <Compile Include="..\WindowsApplication4\Program.cs" Link="Program.cs" />
    <Compile Include="..\WindowsApplication4\Properties\Resources.Designer.cs" Link="Resources.Designer.cs" />
    <Compile Include="..\WindowsApplication4\Properties\Settings.Designer.cs" Link="Settings.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="..\WindowsApplication4\Form1.resx" Link="Form1.resx" />
    <EmbeddedResource Include="..\WindowsApplication4\Properties\Resources.resx" Link="Resources.resx" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\WindowsApplication4\nwind.sqlite" Link="nwind.sqlite">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="..\WindowsApplication4\Properties\Settings.settings" Link="Settings.settings" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="DevExpress.WindowsDesktop.Win" Version="20.1.9-ctp" />
    <PackageReference Include="System.Data.SQLite.Core" Version="1.0.112.2" />
  </ItemGroup>
</Project>
